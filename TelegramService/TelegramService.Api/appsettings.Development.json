{
  "ConnectionStrings": {
    "ServiceUrl": "http://localhost:5010",
    "TelegramBotUrl": "https://api.telegram.org",
    "DockerRedisConnection": "127.0.0.1:6379"
  },
  "TelegramBotConfig": {
    "BotToken": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "<API key here>"
        }
      }
    ]
  }
}
